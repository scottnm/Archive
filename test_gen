all:
	mkdir 1
	touch 1/file1.txt

	mkdir 1/2
	touch 1/2/file2.txt

	mkdir 1/2/3
	touch 1/2/3/file3.txt

	mkdir 1/2/3/4
	touch 1/2/3/4/file4.txt

	printf "\nDIR:\n"
	ls 1/
	ls 1/2
	ls 1/2/3
	ls 1/2/3/4
	printf "\n\n"
	./main 1
	printf "\nDIR:\n"
	ls 1/
	ls 1/2
	ls 1/2/3
	ls 1/2/3/4
	printf "\n\n"

clean:
	rm -rf 1/
